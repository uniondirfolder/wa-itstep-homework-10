@{ 
    Layout = null;
}
@addTagHelper *, Microsoft.AppNetCore.Mvc.TagHelpers


<!DOCTYPE html>

<html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Conference Registration - Welcome!</title>
